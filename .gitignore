!contracts/.env.example

# Backend
backend/node_modules/
backend/.env
backend/dist/
backend/build/
# Keep example env
!backend/.env.example

# RSA Keys (IMPORTANT: Never commit private keys!)
*.key
private.key
public.key
backend/private.key
backend/public.key

# Cursor/IDE
.cursorignore
.cursor

# Temporary files
*.swp
*.swo
*~

